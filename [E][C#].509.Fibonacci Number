public class Solution {
    public static int sum = 0;
        public int Fib(int n)
        {
            if (n > 1)
			{
                return Fib(n - 1) + Fib(n - 2);
            }
            if (n == 1)
                return 1;
            if (n == 0) 
                return 0;
            return sum;
        }
}
